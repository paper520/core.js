{
  "name": "@yelloxing/core.js",
  "version": "0.2.6",
  "description": "üê† A modern JavaScript utility library delivering modularity, performance, &amp; extras.",
  "main": "dist/core.min.js",
  "module": "dist/core.min.js",
  "sideEffects": false,
  "scripts": {
    "start": "npm install && npm run build",
    "build": "npm run build-core && npm run build-tools",
    "build-core": "npm run build-core:bundle && npm run build-core:babel && npm run build-core:uglify && node ./scripts/prependBanner.js",
    "build-core:bundle": "rollup core.js --file dist/core.es5+.js --format iife --name 'bundle'",
    "build-core:babel": "npx babel dist/core.es5+.js --out-file dist/core.js",
    "build-core:uglify": "uglifyjs dist/core.js -o dist/core.min.js",
    "build-tools:bundle": "rollup test/.temp/index.js --file test/.temp/tools.es5+.js --format iife --name 'bundle'",
    "build-tools:babel": "npx babel test/.temp/tools.es5+.js --out-file test/.temp/tools.js",
    "build-tools": "npm run build-tools:bundle && npm run build-tools:babel",
    "postinstall": "node ./scripts/postinstall.js || echo \"ignore\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yelloxing/core.js.git"
  },
  "keywords": [
    "core",
    "es",
    "render"
  ],
  "author": "ÂøÉÂè∂",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yelloxing/core.js/issues"
  },
  "homepage": "https://github.com/yelloxing/core.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "rollup": "^1.20.0",
    "uglify-js": "^3.6.0"
  }
}
